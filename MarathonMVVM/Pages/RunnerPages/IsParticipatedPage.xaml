<Page x:Class="MarathonMVVM.Pages.RunnerPages.IsParticipatedPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MarathonMVVM.Pages.RunnerPages"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="1000"
      Title="IsParticipatedPage">

    <Grid Background="White">
        <StackPanel VerticalAlignment="Center">
            <Button Margin="0,0,0,30">
                Я новенький
            </Button>

            <Button Command="{Binding RedirectAutorisation}">
                Я уже смешарик
            </Button>
                
        </StackPanel>
    </Grid>
</Page>
